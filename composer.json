{
    "name": "tbcd/excel-utils",
    "type": "library",
    "description": "Easy way to create Excel worksheets, spreadsheets and files",
    "authors": [
        {
            "name": "Thomas Beauchataud"
        }
    ],
    "license": "MIT",
    "keywords": [
        "excel",
        "worksheet",
        "spreadsheet",
        "sheet"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.1.0",
    "require": {
        "php": ">=8.1.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "phpoffice/phpspreadsheet": "^1.19",
        "symfony/filesystem": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "TBCD\\Excel\\": "src/"
        }
    }
}